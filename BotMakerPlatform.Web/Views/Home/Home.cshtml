@model List<IBot>

@{
    ViewBag.Title = "Home";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var bot in Model)
        {
            <tr>
                <th scope="row">@(Model.IndexOf(bot) + 1)</th>
                <td><a href="@Url.Action("Index", "Bots", new { uniquename = bot.UniqueName })">@bot.UniqueName</a></td>
                <td>@bot.Description</td>
            </tr>
        }
    </tbody>
</table>